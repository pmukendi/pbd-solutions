public class MoreVariablesAndPrinting {
    public static void main(String[] args) {
        String myName, myEyes, myTeeth, myHair;
        int myAge, myHeight, myWeight;
        
        myName = "Gucci Ganger";
        myAge = 10000000; 
        myHeight = 2;
        myWeight = 1234; 
        myEyes = "Black";
        myTeeth = "Brown";
        myHair = "blue";
        
        
        System.out.println("Let's talk about " + myName + ".");
        System.out.println("He's " + myHeight + " inches tall.");
        System.out.println("He's " + myWeight + " pounds heavy.");
        System.out.println("Actually, that's really fat.");
        System.out.println("He's got " + myEyes + " eyes and " + myHair + " hair.");
        System.out.println("His teeth are usually " + myTeeth + " depending on the mary juan eh.");
        
        System.out.println("If I add " + myAge + " , " + myHeight + ", and " + myWeight + " I get " + (myAge + myHeight + myWeight) + ".");
        }
        }
      
