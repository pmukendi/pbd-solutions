public class LetterToSomeone {
    public static void main {
        System.out.println("+---------------------------------------------------------+");
        System.out.println("|                                                    #### |");
        System.out.println("|                                                    #### |");
        System.out.println("|                                                    #### |");
        System.out.println("|                                                         |");
        System.out.println("|                                                         |");
        System.out.println("|                              Jimmbo bobo                |");
        System.out.println("|                              213 Gucci HQ               |");
        System.out.println("|                              Edmund, WA 555555          |");
        System.out.println("|                                                         |");
        System.out.println("+---------------------------------------------------------+");
    }
}
